        "test": "cypress run",
        "cypress": "cypress open"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "cypress": "^7.5.0",
        "rollup": "^2.35.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-filesize": "^9.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^7.0.2"
    },
    "dependencies": {
        "@alpinejs/morph": "^3.10.2"
    }
}
